---
;; unknown if this is even valid
body: |
  bb.1.entry:
    successors: %bb.2(0x40000000), %bb.3(0x40000000)
  
    %1:_(s8) = COPY $r0
    %2:_(s8) = COPY $r1
    %5:_(s1) = G_ICMP intpred(eq), %1, %2
    G_BRCOND %5(s1), %bb.2
    G_BR %bb.3
  
  bb.2.if.then:
    successors: %bb.4(0x80000000)
  
    G_BR %bb.4
  
  bb.3.if.else:
    successors: %bb.4(0x80000000)
  
  bb.4.if.end:
    RETURN implicit %5
