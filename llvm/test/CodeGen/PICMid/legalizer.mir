---
name: idk
body: |
  bb.0.entry:
    ;; unknown if this is even valid
    successors: %bb.2(0x40000000), %bb.3(0x40000000)

    %1:_(s8) = COPY $r0
    %2:_(s8) = COPY $r1
    %5:_(s1) = G_ICMP intpred(eq), %1, %2
    G_BRCOND %5(s1), %bb.2
    G_BR %bb.3

  bb.2:
    successors: %bb.4(0x80000000)

    G_BR %bb.4

  bb.3:
    successors: %bb.4(0x80000000)

  bb.4:
    RETURN implicit %5
