# RUN: llc -mtriple=picmid -start-before=legalizer -stop-after=legalizer -verify-machineinstrs -o - %s | FileCheck %s

---
name: add_s8
body: |
  bb.0.entry:
    %0:_(s8) = COPY $r0
    %1:_(s8) = COPY $r1
    %2:_(s8) = G_ADD %0, %1
    RETURN implicit %2
---
name: sub_s8
body: |
  bb.0.entry:
    %0:_(s8) = COPY $r0
    %1:_(s8) = COPY $r1
    %2:_(s8) = G_SUB %0, %1
    RETURN implicit %2
